pub fun run(args: [Text]) {
    for arg in args {
        if arg == "qemu" {
            $ qemu-system-i386 -cdrom images/benix.iso -drive format=raw,file=images/benix.img -boot d -m 512M$
                failed {
                    echo "Failed to run qemu"
                }
        }
    }
}

main(args) {
    run(args)
}